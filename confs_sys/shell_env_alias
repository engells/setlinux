#alias vimsd='vim -S $HOME/.vim/sessions/default.vim'
#alias aptcc='sudo apt autoremove; sudo apt remove; sudo apt autoclean; sudo apt clean'

# nautilus scripts
alias clnhis="cat /dev/null > $HOME/.config/zsh/.zhistory"
alias clnpac="sudo pacman -Sc --noconfirm && sudo paccache -rk 1"
alias tmuxx="zsh $HOME/mnt/xcmds/terminal"
