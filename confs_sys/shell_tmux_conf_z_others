# This profile is for my customization
# This file is at $HOME/.confog/z_my_config
# vim:ts=2


# lockscreen ^X x
unbind ^X
bind ^X lock-server
unbind x
bind x lock-server

# quit \
unbind \
bind \ confirm-before "kill-server"

# displays *
unbind *
bind * list-clients

# redisplay ^L l
unbind ^L
bind ^L refresh-client
unbind l
bind l refresh-client

#set -g mouse on


